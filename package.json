        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "npm-run-all": "4.1.5",
        "prettier": "3.2.5",
        "prisma": "5.12.1",
